{
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true,
    "mocha": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "root": true,
  "rules": {
    "no-unused-vars": [2, {"vars": "all", "args": "none"}],
    "eqeqeq": 1,
    "eol-last": 2,
    "quotes": [2, "single"],
    "semi": 1
  }
}
